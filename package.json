    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  }
